{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "canary": {
        "type": "object",
        "properties": {
          "enabled": {
            "type": "boolean",
            "default": false
          },
          "weight": {
            "type": "integer",
            "minimum": 1,
            "maximum": 100,
            "description": "Optional: The weight of traffic to the canary deployment. Must be between 1 and 99.",
            "default": 10
          }
        }
      }
    }
  }
  